<Window x:Class="Personal_Genome_Explorer.PasswordWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Data file password" SizeToContent="WidthAndHeight" ResizeMode="NoResize" Background="#FFECE9D8">
    <Grid Margin="10,10,10,10">
        <StackPanel>
            <Grid Margin="2,2,2,2">
                <Label Margin="2,1,0,0" Name="label2" Tag="{x:Null}" HorizontalAlignment="Left">Password</Label>
                <PasswordBox Name="passwordTextBox" ToolTip="Enter your data file password here" Margin="100,0,100,0" Width="200" />
                <Button Name="cancelButton" HorizontalAlignment="Right" Width="85" IsCancel="True">Cancel</Button>
            </Grid>
            <Grid Margin="2,2,2,2">
                <Button Name="okButton" HorizontalAlignment="Right" Width="85" IsDefault="True">OK</Button>
            </Grid>
        </StackPanel>
    </Grid>
</Window>
