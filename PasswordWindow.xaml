<Window x:Class="Personal_Genome_Explorer.PasswordWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Data file password" Height="111" Width="300" ResizeMode="NoResize" Background="#FFECE9D8">
	<Grid Margin="10,10,10,10">
    <Label Margin="2,0.276666666666699,0,0" Name="label2" Tag="{x:Null}" Height="23.2766666666667" VerticalAlignment="Top" HorizontalAlignment="Left" Width="72">Password</Label>
		<PasswordBox Name="passwordTextBox" ToolTip="Enter your data file password here" Margin="74,0.277,90.5,0" Height="24" VerticalAlignment="Top" />
		<Button Name="cancelButton" HorizontalAlignment="Right" Width="75" Margin="0,35,0,0" Height="24" VerticalAlignment="Top" IsCancel="True">Cancel</Button>
		<Button Name="okButton" Margin="0,0.276666666666699,0,0" Height="24" VerticalAlignment="Top" HorizontalAlignment="Right" Width="75" IsDefault="True">OK</Button>
	</Grid>
</Window>
