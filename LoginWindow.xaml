<Window x:Class="Personal_Genome_Explorer.LoginWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="23andme.com login" Height="111" Width="300" ResizeMode="NoResize" Background="#FFECE9D8">
	<Grid Margin="10,10,10,10">
		<Label HorizontalAlignment="Left" Margin="2,1,0,0" Name="label1" Width="72" Tag="{x:Null}" Height="23.277" VerticalAlignment="Top">Username</Label>
		<TextBox Name="usernameTextBox" ToolTip="Enter your 23andme.com account username here" MaxLines="1" Margin="72,0.277,90.5,0" Height="24" VerticalAlignment="Top"></TextBox>
		<Label Margin="2,0,0,-5.277" Name="label2" Tag="{x:Null}" Height="23.277" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="72">Password</Label>
		<PasswordBox Name="passwordTextBox" ToolTip="Enter your 23andme.com account password here" Margin="72,0,90.5,-6" Height="24" VerticalAlignment="Bottom" />
		<Button Name="cancelButton" HorizontalAlignment="Right" Width="75" Margin="0,35,0,0" Height="24" VerticalAlignment="Top" IsCancel="True">Cancel</Button>
		<Button Name="okButton" Margin="0,0.276666666666699,0,0" Height="24" VerticalAlignment="Top" HorizontalAlignment="Right" Width="75" IsDefault="True">OK</Button>
	</Grid>
</Window>
